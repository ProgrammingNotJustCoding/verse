{
  "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
  "tasks.max": "1",
  "topics": "lk.transcriptions",
  "connection.url": "jdbc:postgresql://postgres:5432/verse_db",
  "connection.user": "verse",
  "connection.password": "verse_secret",
  "auto.create": "false",
  "auto.evolve": "false",
